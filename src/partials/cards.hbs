{{#each this}}
  <li class="gallery-item">
    <div class="photo-card">
      <a href="{{largeImageURL}}">
        <img class="gallery-image" src="{{webformatURL}}" alt="{{tags}}" data-src="{{largeImageURL}}" />
      </a>

      <div class="stats">
        <p class="stats-item">
          <i class="material-icons">thumb_up</i>
          {{likes}}
        </p>
        <p class="stats-item">
          <i class="material-icons">visibility</i>
          {{views}}
        </p>
        <p class="stats-item">
          <i class="material-icons">comment</i>
          {{comments}}
        </p>
        <p class="stats-item">
          <i class="material-icons">cloud_download</i>
          {{downloads}}
        </p>
      </div>
    </div>
  </li>
  {{/each}}